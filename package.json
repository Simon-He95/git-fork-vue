{
  "name": "@simon_he/git-fork-vue",
  "type": "module",
  "version": "0.0.15",
  "packageManager": "pnpm@10.10.0",
  "description": "git-fork",
  "author": "Simon He",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git + git@github.com:Simon-He95/git-fork-vue.git"
  },
  "bugs": {
    "url": "https://github.com/Simon-He95/git-fork-vue/issues"
  },
  "keywords": [
    "git-fork-vue",
    "vue"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.umd.js"
    }
  },
  "main": "./dist/index.umd.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "pnpm run -C playground dev",
    "play:build": "pnpm run -C playground build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest --dir test -u",
    "ptest": "pnpm run -C playground test -u",
    "preview": "pnpm run -C playground preview",
    "test:e2e": "cypress open",
    "prepublishOnly": "nr build",
    "release": "bumpp --commit --tag --push && git push origin --tags -f && npm publish"
  },
  "dependencies": {
    "@vueuse/core": "^8.9.4",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.12.0",
    "@types/node": "^17.0.45",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vitejs/plugin-vue-jsx": "^2.1.1",
    "bumpp": "^7.2.0",
    "eslint": "^9.25.1",
    "typescript": "^4.9.5",
    "vite": "^6.3.4",
    "vite-plugin-css-injected-by-js": "^2.4.0",
    "vite-plugin-dts": "^1.7.3",
    "vitest": "^3.1.2"
  }
}
